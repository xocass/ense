<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane prefHeight="59.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gal.usc.etse.sharecloud.guiController.cTemplateRecentTrack">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="80.0" prefWidth="530.0" spacing="30.0">
         <children>
            <ImageView fx:id="image" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" />
            <Label fx:id="name" text="Label" />
            <Label fx:id="artists" text="Label" />
            <Label fx:id="duration" text="Label" />
            <ImageView fx:id="preview" fitHeight="80.0" fitWidth="80.0" onMouseClicked="#play" pickOnBounds="true" preserveRatio="true" />
         </children>
      </HBox>
   </children>
</AnchorPane>
